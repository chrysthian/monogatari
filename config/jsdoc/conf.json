{
  "opts": {
    "template": "config/jsdoc/template",
    "destination": "dist/docs/api",
    "recurse": true,
    "readme": "README.md"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "includeDate": false
    }
  }
}